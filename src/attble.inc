!
!  *** / A T T B L E / ***
!
!  BUFFER TO HOLD ONE PAGE FROM THE ATTRIBUTE RELATION
!
      COMMON /ATTBLE/ ATTBUF(ZF1),CANAME,CRNAME,CRSTRT,CROW,LROW,NAROW
      COMMON /ATTBLE1/ATTMOD,APBUF
      INTEGER ATTBUF
      INTEGER CANAME(Z)
      INTEGER CRNAME(Z)
      INTEGER CRSTRT
      INTEGER CROW
      INTEGER LROW
      INTEGER NAROW
      INTEGER ATTMOD
      INTEGER APBUF
      INTEGER ATTBLE(ZATTL,ZATTR)
      EQUIVALENCE (ATTBUF(2),ATTBLE(1,1))
!
!  VARIABLE DEFINITIONS:
!         ATTBUF--BUFFER FOR ONE PAGE FROM THE ATTRIBUTE RELATION
!         ATTBLE--EQUIVALENCE ARRAY FOR EASIER USE OF ATTBUF
!         CANAME--CURRENT ATTRIBUTE NAME
!         CRNAME--CURRENT RELATION NAME
!         CRSTRT--CURRENT START IN ATTBUF FOR CRNAME
!         CROW----NEXT ROW IN ATTBLE TO GET
!         LROW----LAST ROW SENT IN TUPLEA
!         NAROW---NEXT AVAILABLE ROW FOR ADDING A TUPLE
!         ATTMOD--MODIFICATION FLAG - O MEANS NO, 1 MEANS YES
!         APBUF---ATTRIBUTES PER ATTBUF PAGE
!

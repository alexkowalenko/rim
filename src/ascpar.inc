!
!  *** / A S C P A R / ***
!
!  ASCII-TEXT PARAMETERS ( MUST BE VARIABLES )
!
      COMMON /ASCPAR/ ABLANK,BLANK(Z),NONE(Z)
      COMMON /ASCPAR1/ KDBHDR(Z)
      COMMON /ASCPAR2/ KARRC(Z),KARDT(Z),KANAM(Z)
      COMMON /ASCPAR3/ KANUM(Z),KAAOR(Z),KAAN1(Z),KARN1(Z),KAOPR(Z),KATYP(Z)
      COMMON /ASCPAR4/ KAAN2(Z),KARN2(Z),KAVAL(Z),KAXXX(Z)
      COMMON /ASCPAR5/ KZHPDB(Z),KZHPRL(Z),KZHPKY(Z),KZHPSK(Z),KZHPTX(Z)
      COMMON /ASCPAR6/ KDATFM,KTIMFM
      COMMON /ASCPAR7/ ASMTXT(12)

      INTEGER :: ABLANK, BLANK, NONE, KDBHDR, KARRC, KARDT ,KANAM, KANUM
      INTEGER :: KAAOR, KAAN1, KARN1, KAOPR, KATYP, KAAN2, KARN2, KAVAL
      INTEGER :: KAXXX, KZHPDB, KZHPRL, KZHPKY, KZHPSKZ, KZHPTX, KDATFM
      INTEGER :: KTIMFM, ASMTXT, KZHPSK
!
!  PARAMETER DEFINITIONS:
!
!     ABLANK --- A ASCII-CHAR BLANK
!     BLANK ---- ASCII-TEXT (Z) BLANKS
!     NONE ----- PASSWORD FOR 'NO PASSWORD'
!
!     KDBHDR --- ASCII-TEXT DATABASE IDENT
!
!     KLKREL --- LINK RELATION NAME
!
!     KARDT-KAXXX - RULE RELATION AND ATTRIBUTE NAMES
!
!     HELP TEXT DATABASE NAMES
!
!     KZHPDB   - HELP DATABASE NAME
!     KZHPRL   - HELP TEXT RELATION NAME
!     KZHPKY   - HELP TEXT KEY ATTRIBUTE NAME
!     KZHPSK   - HELP TEXT SUBKEY ATTRIBUTE NAME
!     KZHPTX   - HELP TEXT TEXT ATTRIBUTE NAME
!
!     DEFAULT FORMATS
!
!     KDATFM  -- DATE PRINT FORMAT
!     KTIMFM  -- TIME PRINT FORMAT
!
!     ASMTXT  -- ASCII-TEXT OF MONTH NAMES (3-CHARS)
!

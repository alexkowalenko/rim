C
C     *** TMPA ***
C
C     TEMPORARY ATTRIBUTE ARRAY / DURING RELATION DEFINITION
C
C           1    = ATTRIBUTE NAME (LENGTH = Z)
C           2    = TYPE
C           3    = LENGTH
C           4    = KEY
C           5    = FORMAT
C
      PARAMETER (TMPA1=1,TMPA2=TMPA1+Z,TMPA3=TMPA2+1,TMPA4=TMPA3+1)
      PARAMETER (TMPA5=TMPA4+1)
      PARAMETER (TMPAL=TMPA5)
C

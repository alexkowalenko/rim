      INTEGER, PARAMETER :: ASPACE=32
      INTEGER, PARAMETER :: LA=97,LZ=122,UA=65,UZ=90,USCOR=95
      INTEGER, PARAMETER :: UECH=69,LECH=101
      INTEGER, PARAMETER :: U0=48,U9=57,PLSIGN=43,MNSIGN=45,DECIM=46
      INTEGER, PARAMETER :: TMSIGN=42,DVSIGN=47
      INTEGER, PARAMETER :: ATSIGN=64,PCSIGN=37
      INTEGER, PARAMETER :: EQSIGN=61,GTSIGN=62,LTSIGN=60,SQUOTE=39
      INTEGER, PARAMETER :: DQUOTE=34
      INTEGER, PARAMETER :: ASSTAR=42,ASPCNT=37
      INTEGER, PARAMETER :: ASCOLN=58
      INTEGER, PARAMETER :: ASRCLL=82,ASCLC=67,ASLXEI=60,ASLXEE=62

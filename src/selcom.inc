!
!
      COMMON /SELCOM/ NUMATT, COL1(ZMSEL),ITEMW(ZMSEL),CURPOS(ZMSEL)
      COMMON /SELCOM1/ NUMCOL(ZMSEL),FORMT(ZMSEL),ATYPE(ZMSEL)
      COMMON /SELCOM2/ LEN(ZMSEL),ROWD(ZMSEL),COLD(ZMSEL),FP(ZMSEL)
      COMMON /SELCOM3/ SINGLE(ZMSEL),VAR(ZMSEL),SUMFLG(ZMSEL)
      COMMON /SELCOM4/ LIN1(ZMSEL),SFUNCT,FUNCT(ZMSEL),SGRPBY,SLNKFL
      COMMON /SELCOM5/ LNKFL(ZMSEL),SLVMHD,TITLE(ZPRINW),MINUS(ZPRINW)
      COMMON /SELCOM6/ LINE(ZPRINW),SLNKNM(Z,ZMSLK)

      INTEGER, PARAMETER :: ZSELCM=1+12*ZMSEL
      LOGICAL :: VAR
      LOGICAL :: SUMFLG
      LOGICAL :: SFUNCT
      LOGICAL :: SLVMHD
      INTEGER :: NUMATT, COL1, ITEMW, CURPOS, NUMCOL, FORMT, ATYPE, LEN
      INTEGER :: ROWD, COLD, FP, SINGLE, LIN1, FUNCT, SGRPBY, SLNKFL
      INTEGER :: LNKFL, TITLE, MINUS,LINE, SLNKNM
!
!  VARIABLE DEFINITIONS:
!     NUMATT....NUMBER OF ATTRIBUTES
!     COL1......FIRST COLUMN IN LINE TO PRINT INTO
!     ITEMW.....NUMBER OF COLUMNS PER ITEM
!     CURPOS....CURRENT POSITION IN ATTRIBUTE (FOR PARAGRAPHING)
!     NUMCOL....NUMBER OF COLUMNS AVAILABLE FOR THIS ATTRIBUTE
!     PFORMT....FORMAT INPUT BY USER OR ZERO
!     TRUNC.....TRUE IFF ROWS ARE TO BE TRUNCATED  AS PARAGRAPHS
!     ATYPE.....ATTRIBUTE TYPE
!     LEN.......ATTRIBUTE LENGTH (WORDS EXCEPT TEXT IS CHARACTERS)
!     ROWD......ROW DIMENSION
!     COLD......COLUMN DIMENSION
!     VAR.......TRUE IFF VARIABLE ATTRIBUTE
!     FP........POINTER TO CELL IF FIXED POTION OF TUPLE
!     SINGLE....CONTAINS ROW/COL OR ENTRY FOR MAT(I,J)/VEC(J) ITEMS
!     SLVMHD....TRUE IF SHOULD PRINT VAR VEC AND MAT HEADERS
!     LINE......OUTPUT LINE SPACE (ASCII-TEXT)
!     TITLE.....TITLE LINE
!     MINUS.....LINE WITH DASHES
!     SLNKFL....NUMBER OF INDEPENDENT LINKS
!     LNKFL.....0=NO LINK, ELSE INDEX INTO LNKNAM
!     SLNKNM....LINK NAMES
!
!     ZSELC1....LENGTH OF COMMON THROUGH SUMFLG (FOR PROGRAM MODE)

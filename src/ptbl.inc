!
!     *** PTBL ***
!
!     POINTER TABLE FOR ALGEBRA COMMANDS
!
!        PTBL1 -- ATTRIBUTE NAME
!        PTBL2 -- ATTRIBUTE LOCATION IN RELATION 1
!        PTBL3 -- ATTRIBUTE LOCATION IN RELATION 2
!        PTBL4 -- ATTRIBUTE LOCATION IN RELATION 3
!        PTBL5 -- LENGTH IN WORDS
!        PTBL6 -- ATTRIBUTE TYPE
!        PTBL7 -- ATTRIBUTE FORMAT
!
      INTEGER, PARAMETER :: PTBL1=1,PTBL2=PTBL1+Z,PTBL3=PTBL2+1
      INTEGER, PARAMETER :: PTBL4=PTBL3+1
      INTEGER, PARAMETER :: PTBL5=PTBL4+1,PTBL6=PTBL5+1,PTBL7=PTBL6+1
      INTEGER, PARAMETER :: PTBLL=PTBL7
!
